{
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "build"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/better-docs"
  }
}
